    "start": "npm run dev",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:update": "vitest -u",
    "ts-check": "tsc  -p tsconfig-check.json"
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write --no-error-on-unmatched-pattern"
    ],
    "*.{css,less}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
